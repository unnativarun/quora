<p id="notice"><%= notice %></p>

<h1>Question</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= link_to question.title, question_path(question), class: "question" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
